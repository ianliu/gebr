<?xml version="1.0" encoding="utf-8"?>

<glossary>
	<glossdiv>
		<title>C</title>
		<glossentry id="Client">
			<glossterm>Client</glossterm>
			<glossdef>
				<para>The machine which is running GêBR.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>D</title>
		<glossentry id="Dictionary">
			<glossterm>Dictionary of variables</glossterm>
			<glossdef>
				<para>Where the variables are defined.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>L</title>
		<glossentry id="Line">
			<glossterm>""Line""</glossterm>
			<glossdef>
				<para>Set that organizes and clusters seismic flows.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>F</title>
		<glossentry id="Flow">
			<glossterm>Flow</glossterm>
			<glossdef>
				<para>A sequence of programs that may transform the input data.
				</para>
				<glossseealso otherterm="Program">Program</glossseealso>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>J</title>
		<glossentry id="Job">
			<glossterm>Job</glossterm>
			<glossdef>
				<para>A flow submitted to execution, thus, a running flow.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>M</title>
		<glossentry id="Maestro">
			<glossterm>"Maestro"</glossterm>
			<glossdef>
				<para>The manager machine of resources of the other machines running
					GêBR (clients).</para>
				<glossseealso otherterm="" Maestro ">"Maestro"
				</glossseealso>
			</glossdef>
		</glossentry>
		<glossentry id="Menu">
			<glossterm>""Menu""</glossterm>
			<glossdef>
				<para>Representation of a single program or a set of programs.
				</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>P</title>
		<glossentry id="Parameter">
			<glossterm>Parameter</glossterm>
			<glossdef>
				<para>Values passed to programs to change their behavior.</para>
				<glossseealso otherterm="Program">Program</glossseealso>
			</glossdef>
		</glossentry>
		<glossentry id="Program">
			<glossterm>Program</glossterm>
			<glossdef>
				<para>Operation that generates a specific result according to
					user-defined parameters.</para>
				<glossseealso otherterm="Parameter">Parameter</glossseealso>
			</glossdef>
		</glossentry>
		<glossentry id="Project">
			<glossterm>Project</glossterm>
			<glossdef>
				<para>Set that organizes and clusters seismic lines.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>S</title>
		<glossentry id="Snapshot">
			<glossterm>"Snapshot"</glossterm>
			<glossdef>
				<para>Stores all the settings of a flow at the exact moment when
					that is taken.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
	<glossdiv>
		<title>V</title>
		<glossentry id="Variable">
			<glossterm>"Variable"</glossterm>
			<glossdef>
				<para>Storage location associated a name that keeps a value or
					information.</para>
			</glossdef>
		</glossentry>
	</glossdiv>
</glossary>
