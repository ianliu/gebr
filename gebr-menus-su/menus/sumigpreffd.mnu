    <parameters exclusive="0">
      <parameter id="n0">
        <label>Number of total horizontal output samples</label>
        <int>
          <property required="yes">
            <keyword>nxo=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter id="n1">
        <label>Number of shot gathers to be migrated</label>
        <int>
          <property required="yes">
            <keyword>nxshot=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter id="n2">
        <label>Number of depth samples</label>
        <int>
          <property required="yes">
            <keyword>nz=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter id="n3">
        <label>Horizontal sampling interval</label>
        <float>
          <property required="yes">
            <keyword>dx=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter id="n4">
        <label>Depth sampling interval</label>
        <float>
          <property required="yes">
            <keyword>dz=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter id="n5">
        <label>Velocity profile</label>
        <file directory="no">
          <property required="yes">
            <keyword>vfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter id="n6">
        <label>Peak frequency</label>
        <float>
          <property required="no">
            <keyword>fmax=</keyword>
            <value/>
            <default>25</default>
          </property>
        </float>
      </parameter>
      <parameter id="n7">
        <label>Frequency 1</label>
        <float>
          <property required="no">
            <keyword>f1=</keyword>
            <value/>
            <default>5</default>
          </property>
        </float>
      </parameter>
      <parameter id="n8">
        <label>Frequency 2</label>
        <float>
          <property required="no">
            <keyword>f2=</keyword>
            <value/>
            <default>10</default>
          </property>
        </float>
      </parameter>
      <parameter id="n9">
        <label>Frequency 3</label>
        <float>
          <property required="no">
            <keyword>f3=</keyword>
            <value/>
            <default>40</default>
          </property>
        </float>
      </parameter>
      <parameter id="n10">
        <label>Frequency 4</label>
        <float>
          <property required="no">
            <keyword>f4=</keyword>
            <value/>
            <default>50</default>
          </property>
        </float>
      </parameter>
      <parameter id="n11">
        <label>Number of zero traces padded on left</label>
        <int>
          <property required="no">
            <keyword>lpad=</keyword>
            <value/>
            <default>9999</default>
          </property>
        </int>
      </parameter>
      <parameter id="n12">
        <label>Number of zero traces padded on right</label>
        <int>
          <property required="no">
            <keyword>rpad=</keyword>
            <value/>
            <default>9999</default>
          </property>
        </int>
      </parameter>
      <parameter id="n13">
        <label>Print some useful information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value/>
            <default/>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
