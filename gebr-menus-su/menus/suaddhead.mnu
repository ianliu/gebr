      </flag></parameter>
      <parameter id="n2"><label>Trace sorting code</label><range min="1" max="4" inc="1" digits=""><property required="no"><keyword>tsort=</keyword><value></value><default>3</default></property></range></parameter>
      <parameter id="n3"><label>Number of data traces per record</label><int><property required="no"><keyword>ntrpr=</keyword><value></value><default></default></property>
	
	
	
