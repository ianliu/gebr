      <string required="no">
        <keyword>key=</keyword>
        <label>key header word to stack on</label>
        <value default="cdp"/>
      </string>
      <float required="no">
        <keyword>pwr=</keyword>
        <label>raise phase stack to power</label>
        <value default="1.0"/>
      </float>
      <float required="no">
        <keyword>dt=</keyword>
        <label>time sampling intervall in seconds</label>
        <value/>
      </float>
      <float required="no">
        <keyword>sl=</keyword>
        <label>window length in seconds used for smoothing of the phase stack (weights)</label>
        <value/>
      </float>
      <flag>
        <keyword>ps=1</keyword>
        <label>output is phase stack (no-weighting)</label>
        <state default="off"/>
      </flag>
      <flag>
        <keyword>verbose=1</keyword>
        <label>echo additional information</label>
        <state default="off"/>
      </flag>
    </parameters>
  </program>
