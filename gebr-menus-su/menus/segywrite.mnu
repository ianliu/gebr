    <url/><parameters>
      <file required="yes" directory="no"><keyword>tape=</keyword><label>Tape device to use or filename</label><value/></file>
      <flag><keyword>buff=0</keyword><label>For 8mm EXABYTE drives</label><state default="off"/></flag>
      <flag><keyword>conv=0</keyword><label>Don't convert to IBM format</label><state default="off"/></flag>
      <file required="no" directory="no"><keyword>hfile=</keyword><label>EBCDIC card image header file</label><value/></file>
      <file required="no" directory="no"><keyword>bfile=</keyword><label>Binary header file</label><value/></file>
      <int required="no"><keyword>trmin=</keyword><label>First trace to write</label><value/></int>
      <int required="no"><keyword>trmax=</keyword><label>Last trace to write</label><value/></int>
      <flag><keyword>endian=0</keyword><label>Little-endian machines</label><state default="off"/></flag>
      <int required="no"><keyword>errmax=</keyword><label>Allowable number of consecutive tape IO errors</label><value default="0"/></int>
      <string required="no"><keyword>format=</keyword><label>Override value of format in binary header file</label><value/></string>
      <int required="no"><keyword>vblock=</keyword><label>Echoing at each amount of trace</label><value default="50"/></int>
      <flag><keyword>verbose=1</keyword><label>Print some useful information</label><state default="off"/></flag>
    </parameters>
  </program>
