    <url>http://www.cwp.mines.edu/cwpcodes/index.html</url><parameters>
      <float required="no">
        <keyword>a=</keyword>
        <label>alpha (vertical p velocity)</label>
        <value/>
      </float>
      <float required="no">
        <keyword>b=</keyword>
        <label>beta (vertical sv velocity)</label>
        <value/>
      </float>
      <float required="no">
        <keyword>e=</keyword>
        <label>epsilon (horiz p-wave anisotropy)</label>
        <value/>
      </float>
      <float required="no">
        <keyword>d=</keyword>
        <label>delta (strange parameter)</label>
        <value/>
      </float>
      <int required="no">
        <keyword>nangle=</keyword>
        <label>number of angles to compute</label>
        <value/>
      </int>
      <float required="no">
        <keyword>maxangle=</keyword>
        <label>max angle in degrees</label>
        <value/>
      </float>
      <flag>
        <keyword>verbose=1</keyword>
        <label>echoing information</label>
        <state default="off"/>
      </flag>
      <int required="no">
        <keyword>np=</keyword>
        <label>number of slowness</label>
        <value/>
      </int>
      <range min="1" max="14" inc="1" required="no" digits="">
        <keyword>option=</keyword>
        <label>velocity option</label>
        <value default="1"/>
      </range>
      <flag>
        <keyword>normalize=1</keyword>
        <label>normalize</label>
        <state default="off"/>
      </flag>
    </parameters>
  </program>
