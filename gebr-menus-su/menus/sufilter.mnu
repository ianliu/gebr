    <url>http://www.cwp.mines.edu/cwpcodes/index.html</url><parameters>
      <float required="no">
        <keyword>dt=</keyword>
        <label>time sampling interval</label>
        <value/>
      </float>
      <string required="no">
        <keyword>f=</keyword>
        <label>array of filter frequencies(HZ)</label>
        <value/>
      </string>
      <string required="no">
        <keyword>amps=</keyword>
        <label>array of filter amplitudes</label>
        <value/>
      </string>
      <flag>
        <keyword>verbose=1</keyword>
        <label>echoing useful information</label>
        <state default="off"/>
      </flag>
    </parameters>
  </program>
