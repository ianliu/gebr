<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flow version="0.1.0">
  <filename>sufrac.mnu</filename>
  <title>SU frac</title>
  <description>Fraction derivative or integral</description>
  <help><![CDATA[
    Take general (fractional) time derivative or integral of
    data, plus a phase shift. Input is TIME DOMAIN data.
    ]]>
  </help>
  <author>GÃªBR core team</author>
  <email>gebr@users.sf.net</email>
  <category>test</category>
  <io>
    <input></input>
    <output></output>
    <error></error>
  </io>
  <program stdin="yes" stdout="yes" stderr="no" status="unconfigured">
    <menu index="0">sufrac.menu</menu>
    <title>SU frac</title>
    <binary>sufrac</binary>
    <description>Fraction derivative or integral</description>
    <help><![CDATA[
      Take general (fractional) time derivative or integral of
      data, plus a phase shift. Input is TIME DOMAIN data.
      ]]>
    </help>
    <parameters>
      <float required="no">
	<keyword>power</keyword>
	<label>
	  exponent of (-i*omega)
	</label>
	<value default="0"></value>
      </float>
      <int required="no">
	<keyword>sign</keyword>
	<label>
	  sign in front of i * omega
	</label>
	<value default="-1"></value>
      </int>
      <float required="no">
	<keyword>dt</keyword>
	<label>
	  time sample interval (in seconds)
	</label>
	<value></value>
      </float>
      <float required="no">
	<keyword>phasefac</keyword>
	<label>
	  phase shift by phase=phasefac*PI
	</label>
	<value default="0"></value>
      </float>
    </parameters>
  </program>
</flow>
