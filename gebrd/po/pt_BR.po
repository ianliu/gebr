#: ../src/client.c:99
#, c-format
msgid "Connection error '%s' on client '%s'"
msgstr "Erro '%s' na conexão com o servidor '%s'"

#: ../src/gebrd.c:85
msgid "Server quited"
msgstr "Servidor finalizado"

#: ../src/job.c:102
#, c-format
msgid "Required parameter '%s' of program '%s' not provided.\n"
msgstr "O parâmetro '%s' do programa '%s' não foi fornecido.\n"

#: ../src/job.c:121
#, c-format
msgid "Unknown parameter type.\n"
msgstr "Tipo de parâmetro desconhecido.\n"

#: ../src/job.c:191
#, c-format
msgid "Job '%s' sent output not in UTF-8."
msgstr "Tarefa '%s' não enviou a saida no formato UTF-8"

#: ../src/job.c:516
#, c-format
msgid ""
"DTD specified. The <DOCTYPE ...> must not appear in XML.\n"
"libgeoxml will find the appopriated DTD installed from version.\n"
msgstr ""
"DTD especificado. <DOCTYPE ...> não deve aparecer no XML.\n"
"libgeoxml encontrará o DTD apropriado a partir da versão.\n"

#: ../src/job.c:521
#, c-format
msgid ""
"Invalid document. It has a sintax error or doesn't match the DTD.\n"
"In this case see the errors above.\n"
msgstr ""
"Documento inválido. Ele contém um erro de sintaxe ou não respeita o DTD.\n"
"Neste caso, veja os erros acima.\n"

#: ../src/job.c:526
#, c-format
msgid ""
"Can't access file. The file doesn't exist or there is not enough permission "
"to read it.\n"
msgstr ""
"Impossível acessar o arquivo. Ele não existe ou não há permissão de "
"leitura.\n"

#: ../src/job.c:531
#, c-format
msgid ""
"Can't access dtd. The file's DTD couldn't not be found.\n"
"It may be a newer version not support by this version of libgeoxml or there "
"is an installation problem.\n"
msgstr ""
"Impossível acessar o DTD. O arquivo de DTD não pode ser encontrado.\n"
"Pode ser uma versão mais nova, não suportada por esta versão da libgeoxml\n"
"ou então um problema de instalação.\n"

#: ../src/job.c:537
#, c-format
msgid ""
"Not enough memory. The library stoped after an unsucessful memory "
"allocation.\n"
msgstr ""
"Sem memória suficiente. A biblioteca abortou depois de uma alocação de "
"memória mal-sucedida.\n"

#: ../src/job.c:541
#, c-format
msgid ""
"Unspecified error %d.\n"
"See library documentation at http://sites.google.com/site/gebrproject.\n"
msgstr ""
"Erro não especificado %d.\n"
"Veja a documentação da biblioteca em http://sites.google.com/site/"
"gebrproject.\n"

#: ../src/job.c:554
#, c-format
msgid "Empty flow.\n"
msgstr "Fluxo vazio.\n"

#: ../src/job.c:562
#, c-format
msgid "%u) Skiping disabled/unconfigured program '%s.'\n"
msgstr "%u) Desconsiderando programa '%s' desabilitado/não configurado.\n"

#: ../src/job.c:568
#, c-format
msgid "No configured programs.\n"
msgstr "Sem programas configurados.\n"

#: ../src/job.c:578
#, c-format
msgid "No input file selected.\n"
msgstr "Sem arquivo de entrada selecionado.\n"

#: ../src/job.c:585
#, c-format
msgid "Input file %s not present or not accessable.\n"
msgstr "Arquivo de entrada %s não encontrado ou não acessível.\n"

#: ../src/job.c:603 ../src/job.c:672
#, c-format
msgid "Write permission to %s not granted.\n"
msgstr "Permissão de escrita para %s não concedida.\n"

#: ../src/job.c:619
#, c-format
msgid "%u) Skiping disabled/unconfigured program '%s'.\n"
msgstr "%u) Desconsiderando programa '%s' desabilidato/não configurado.\n"

#. Previous does not write to stdin but current expect something
#: ../src/job.c:635
#, c-format
msgid "Broken flow before %s (no input).\n"
msgstr "Fluxo interrompido depois de %s (sem entrada).\n"

#. Previous does write to stdin but current does not carry about
#: ../src/job.c:639
#, c-format
msgid "Broken flow before %s (unexpected output).\n"
msgstr "Fluxo interrompido antes de %s (saída inesperada).\n"

#: ../src/job.c:665
#, c-format
msgid "No error file selected; error output merged with standard output.\n"
msgstr ""
"Sem arquivo de erro selecionado; saída de erro será mesclada com a saída "
"padrão.\n"

#: ../src/job.c:668
#, c-format
msgid "Proceeding without output file.\n"
msgstr "Prosseguindo sem arquivo de saída.\n"

#: ../src/job.c:790
#, c-format
msgid "Cannot submit job to MOAB server.\n"
msgstr "Não foi possível submeter a tarefa ao servidor MOAB.\n"

#: ../src/job.c:796
#, c-format
msgid "Cannot submit job to MOAB server: %s.\n"
msgstr "Não foi possível submeter a tarefa ao servidor MOAB: %s.\n"

#: ../src/job.c:806
#, c-format
msgid "Cannot get MOAB job id.\n"
msgstr "Não foi possível obter a identificação da tarefa no MOAB.\n"

#: ../src/job.c:964 ../src/job.c:968
#, c-format
msgid "Cannot cancel job at MOAB server.\n"
msgstr "Não foi possível cancelar a tarefa no servidor MOAB.\n"

#: ../src/main.c:35
msgid "Run server in interactive mode, not as a daemon"
msgstr "Executando servidor em modo iterativo e não com daemon"

#: ../src/main.c:37
msgid "Show GeBR daemon version"
msgstr "Exibe a versão do daemon"

#: ../src/main.c:50
msgid "GeBR daemon"
msgstr "Daemon GeBR"

#: ../src/main.c:58
#, c-format
msgid "%s: syntax error\n"
msgstr "%s: erro de sintaxe\n"

#: ../src/main.c:59
#, c-format
msgid "Try %s --help\n"
msgstr "Tente %s --help\n"

#: ../src/server.c:73
msgid "Could not access GêBR configuration directories.\n"
msgstr "Não foi possível acessar os diretórios de configuração da GêBR.\n"

#: ../src/server.c:95
msgid "Cannot run interactive server, GêBR daemon is already running"
msgstr ""
"Não é possível executar o servidor iterativavmente. Daemon GêBR já está em "
"execução"

#: ../src/server.c:115
msgid "Could not listen for connections.\n"
msgstr "Não foi possível escutar por conexões.\n"

#: ../src/server.c:123
msgid "Could not write run file."
msgstr "Não foi possível escrever arquivo de trava de execução."

#: ../src/server.c:147
#, c-format
msgid "Server started at %u port"
msgstr "Servidor inicializado na porta %u"

#: ../src/server.c:158
msgid "Could not init server. Quiting..."
msgstr "Não foi possível inicializar o servidor. Abortando..."

#~ msgid "%u) Skipped disabled/unconfigured program '%s'.\n"
#~ msgstr "%u) Desconsiderando programa '%s' desabilidato/não configurado.\n"

#~ msgid "%s\n"
#~ msgstr "%s\\n"
