#: ../src/client.c:100
#, c-format
msgid "Connection error '%d' on client '%s'"
msgstr "Erro '%d' na conexão com o cliente '%s'"

#: ../src/gebrd.c:94
msgid "Server quit."
msgstr "Servidor finalizado."

#: ../src/job.c:105
#, c-format
msgid "Required parameter '%s' of program '%s' not provided.\n"
msgstr "O parâmetro '%s' do programa '%s' não foi fornecido.\n"

#: ../src/job.c:124
#, c-format
msgid "Unknown parameter type.\n"
msgstr "Tipo de parâmetro desconhecido.\n"

#: ../src/job.c:194
#, c-format
msgid "Job '%s' sent output not in UTF-8."
msgstr "Tarefa '%s' não enviou a saida no formato UTF-8"

#: ../src/job.c:527
#, c-format
msgid "Empty flow.\n"
msgstr "Fluxo vazio.\n"

#: ../src/job.c:535 ../src/job.c:613
#, c-format
msgid "%u) Skiping disabled/unconfigured program '%s'.\n"
msgstr "%u) Desconsiderando programa '%s' desabilitado/não configurado.\n"

#: ../src/job.c:541
#, c-format
msgid "No configured programs.\n"
msgstr "Sem programas configurados.\n"

#: ../src/job.c:551
#, c-format
msgid "No input file selected.\n"
msgstr "Sem arquivo de entrada selecionado.\n"

#: ../src/job.c:558
#, c-format
msgid "Input file %s not present or not accessable.\n"
msgstr "Arquivo de entrada %s não encontrado ou não acessível.\n"

#: ../src/job.c:575
#, c-format
msgid "Requested MPI (%s) is not supported by this server.\n"
msgstr "MPI (%s) solicitado não é suportado pelo servidor.\n"

#: ../src/job.c:597 ../src/job.c:695
#, c-format
msgid "Write permission to %s not granted.\n"
msgstr "Permissão de escrita para %s não concedida.\n"

#. Previous does not write to stdin but current expect something
#: ../src/job.c:644
#, c-format
msgid "Broken flow before %s (no input).\n"
msgstr "Fluxo interrompido depois de %s (sem entrada).\n"

#. Previous does write to stdin but current does not carry about
#: ../src/job.c:648
#, c-format
msgid "Broken flow before %s (unexpected output).\n"
msgstr "Fluxo interrompido antes de %s (saída inesperada).\n"

#: ../src/job.c:688
#, c-format
msgid "No error file selected; error output merged with standard output.\n"
msgstr ""
"Sem arquivo de erro selecionado; saída de erro será mesclada com a saída "
"padrão.\n"

#: ../src/job.c:691
#, c-format
msgid "Proceeding without output file.\n"
msgstr "Prosseguindo sem arquivo de saída.\n"

#: ../src/job.c:814
#, c-format
msgid "Cannot submit job to MOAB server.\n"
msgstr "Não foi possível submeter a tarefa ao servidor MOAB.\n"

#: ../src/job.c:820
#, c-format
msgid "Cannot submit job to MOAB server: %s.\n"
msgstr "Não foi possível submeter a tarefa ao servidor MOAB: %s.\n"

#: ../src/job.c:830
#, c-format
msgid "Cannot get MOAB job id.\n"
msgstr "Não foi possível obter a identificação da tarefa no MOAB.\n"

#: ../src/job.c:994 ../src/job.c:998
#, c-format
msgid "Cannot cancel job at MOAB server.\n"
msgstr "Não foi possível cancelar a tarefa no servidor MOAB.\n"

#: ../src/main.c:36
msgid "Run server in interactive mode, not as a daemon"
msgstr "Executando servidor em modo iterativo e não com daemon"

#: ../src/main.c:38
msgid "Show GeBR daemon version"
msgstr "Exibe a versão do daemon"

#: ../src/main.c:47
msgid "GeBR daemon"
msgstr "Daemon GeBR"

#: ../src/main.c:53
#, c-format
msgid "%s: syntax error\n"
msgstr "%s: erro de sintaxe\n"

#: ../src/main.c:54
#, c-format
msgid "Try %s --help\n"
msgstr "Tente %s --help\n"

#: ../src/server.c:71
#, c-format
msgid "%s:%d: Could not access GêBR configuration directories.\n"
msgstr ""
"%s:%d: Não foi possível acessar os diretórios de configuração da GêBR.\n"

#: ../src/server.c:104
msgid "Cannot run interactive server, GêBR daemon is already running"
msgstr ""
"Não é possível executar o servidor iterativavmente. Daemon GêBR já está em "
"execução"

#: ../src/server.c:125
msgid "Could not listen for connections.\n"
msgstr "Não foi possível escutar por conexões.\n"

#: ../src/server.c:133
msgid "Could not write run file."
msgstr "Não foi possível escrever arquivo de trava de execução."

#: ../src/server.c:153
#, c-format
msgid "Server started at %u port"
msgstr "Servidor inicializado na porta %u"

#: ../src/server.c:165
msgid "Could not init server. Quiting..."
msgstr "Não foi possível inicializar o servidor. Abortando..."
