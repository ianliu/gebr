#
# The following command line was used to generate the file
# cmdline.c and cmdline.h
#
# gengetopt < gebr.ggo --file-name=cmdline
#

package "GÍBR"
version "1.0"

purpose "Seismic processing environment"

option "name" - "User name" string no
option "email" - "User email" string no
option "usermenus" - "User local menus dir" string yes
option "data" - "Data user dir" string yes
option "editor" - "HTML capable editor" string no
option "browser" - "HTML browser" string no
option "width" - "Window width" int default="700" no
option "height" - "Window height" int default="400" no
option "logexpand" - "Log exapander state" flag off
option "server" - "Server hostname" string no multiple