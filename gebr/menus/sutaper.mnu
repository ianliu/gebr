    <parameters>
      <int required="no">
        <keyword>ntr=</keyword>
        <label>number of traces</label>
        <value/>
      </int>
      <int required="no">
        <keyword>tr1=</keyword>
        <label>number of traces on the begining</label>
        <value/>
      </int>
      <int required="no">
        <keyword>tr2=</keyword>
        <label>number of to be tapered at the end</label>
        <value/>
      </int>
      <float required="no">
        <keyword>min=</keyword>
        <label>minimum amplitude factor</label>
        <value/>
      </float>
      <float required="no">
        <keyword>tbeg=</keyword>
        <label>length of taper at trace start</label>
        <value/>
      </float>
      <float required="no">
        <keyword>tend=</keyword>
        <label>length of taper at trace end</label>
        <value/>
      </float>
      <range min="1" max="4" inc="1" required="no">
        <keyword>taper=</keyword>
        <label>type of taper</label>
        <value default="1" />
      </range>
    </parameters>
  </program>
