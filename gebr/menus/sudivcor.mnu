    <parameters>
      <string required="no">
        <keyword>trms=</keyword>
        <label>times corresponding to rms velocities below</label>
        <value default="0.0"/>
      </string>
      <string required="no">
        <keyword>vrms=</keyword>
        <label>interval velocities corresponding to times above</label>
        <value default="1500.00"/>
      </string>
      <file required="no" directory="no">
        <keyword>vfile=</keyword>
        <label>file containing velocities vrms(t)</label>
        <value/>
      </file>
    </parameters>
  </program>
