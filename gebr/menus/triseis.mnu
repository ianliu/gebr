    <parameters>
      <file required="yes" directory="no">
        <keyword>modelfile=</keyword>
        <label>file of the model content</label>
        <value />
      </file>
      <file required="yes" directory="no">
        <keyword>seisfile=</keyword>
        <label>file of the result content</label>
        <value />
      </file>
      <string required="yes">
        <keyword>xs=</keyword>
        <label>x coordinates of source surface</label>
        <value />
      </string>
      <string required="yes">
